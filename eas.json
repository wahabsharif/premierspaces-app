{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "environment": "production",
      "releaseChannel": "production",
      "distribution": "store",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "channel": "production",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "development": {
      "environment": "development",
      "developmentClient": true,
      "releaseChannel": "development",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "development",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "preview": {
      "environment": "preview",
      "channel": "preview",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    }
  },
  "submit": {
    "production": {}
  },
  "updates": {

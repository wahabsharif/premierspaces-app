{
  "expo": {
    "name": "Premier Spaces",
    "slug": "premier-spaces",
    "version": "1.2.2",
    "orientation": "portrait",
    "icon": "./assets/touch-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff",
      "translucent": false,
      "hidden": false
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses Face ID to secure your data."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/touch-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.premierspaces",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "1af780ca-d5bf-44fa-983f-7b7e22eb1962"
      }
    },
    "owner": "envbyte",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1af780ca-d5bf-44fa-983f-7b7e22eb1962"
    }
  }
}
